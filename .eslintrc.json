{
  "extends": "airbnb-base",
  "rules": {
    /* (off for dev) */
    "no-console":"off",
    
    "no-underscore-dangle": 0,
    "no-unused-vars":"warn"
  },
  "env": {
    "browser": true
  },
  // required for optional chaining & null coalescing, 
  // when using airbnb esling config
  "parserOptions": {
    "ecmaVersion": 2020
  }
}